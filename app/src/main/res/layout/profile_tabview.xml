       <RelativeLayout
            android:id="@+id/logoutlayout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/profilescreenheight"
           android:layout_marginStart="@dimen/ten"
            android:layout_marginTop="8dp"
           android:layout_marginEnd="@dimen/ten"
           android:background="@android:color/white"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/faqlayout">

            <ImageView
                android:id="@+id/logoutimg"
                android:layout_width="@dimen/profilescreenheight"
                android:layout_height="match_parent"
                android:scaleType="fitXY"
                android:padding="@dimen/fifteen"
                android:src="@drawable/logout"/>

            <TextView
                android:id="@+id/logout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/ten"
                android:layout_toEndOf="@+id/logoutimg"
                android:text="@string/logout"
                android:fontFamily="@font/comfortaa_medium"
                android:textSize="@dimen/textsize_fourtn" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:src="@drawable/right_arrow" />

        </RelativeLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>